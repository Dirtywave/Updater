{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    // "sourceMap": true,
    "exactOptionalPropertyTypes": true,
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "access/*": [
        "./src/access/*"
      ],
      "assets/*": [
        "./src/assets/*"
      ],
      "boot/*": [
        "./src/boot/*"
      ],
      "components/*": [
        "./src/components/*"
      ],
      "composables": [
        "./src/composables/index.ts"
      ],
      "composables/*": [
        "./src/composables/*"
      ],
      "directives/*": [
        "./src/directives/*"
      ],
      "layouts": [
        "./src/layouts"
      ],
      "layouts/*": [
        "./src/layouts/*"
      ],
      "pages/*": [
        "./src/pages/*"
      ],
      "src/*": [
        "./src/*"
      ],
      "stores/*": [
        "./src/stores/*"
      ],
      "types/*": [
        "./src/types*"
      ],
      "utils/*": [
        "./src/utils/*"
      ]
    }
  }
}